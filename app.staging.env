ENVIRONMENT=staging
API_BASE_URL=https://staging-api.alitaecommerce.com/v1
API_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=your_staging_firebase_api_key
FIREBASE_APP_ID=your_staging_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_staging_firebase_messaging_sender_id
FIREBASE_PROJECT_ID=your_staging_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_staging_firebase_storage_bucket

# Payment Gateways Testing Keys
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
MIDTRANS_CLIENT_KEY=your_staging_midtrans_client_key
MIDTRANS_SERVER_KEY=your_staging_midtrans_server_key

# Feature Flags
ENABLE_SOCIAL_LOGIN=true
ENABLE_APPLE_PAY=true
ENABLE_GOOGLE_PAY=true
ENABLE_LIVE_CHAT=true
ENABLE_LOCATION_SERVICES=true

# App Configuration
APP_NAME=Alita Staging
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0

# Cache Configuration
CACHE_DURATION_PRODUCTS=43200
CACHE_DURATION_CATEGORIES=43200
CACHE_DURATION_USER_DATA=86400

# Logging
ENABLE_DEBUG_LOGGING=true
ENABLE_CRASH_REPORTING=true

# Content Delivery Network
CDN_BASE_URL=https://staging-cdn.alitaecommerce.com