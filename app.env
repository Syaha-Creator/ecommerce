# API Configuration
API_BASE_URL=https://api.alitaecommerce.com/v1
API_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_APP_ID=your_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket

# Authentication
AUTH_CLIENT_ID=your_auth_client_id
AUTH_CLIENT_SECRET=your_auth_client_secret

# Payment Gateway
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
MIDTRANS_CLIENT_KEY=your_midtrans_client_key
MIDTRANS_SERVER_KEY=your_midtrans_server_key

# Analytics
ANALYTICS_TRACKING_ID=your_analytics_tracking_id

# Feature Flags
ENABLE_SOCIAL_LOGIN=true
ENABLE_APPLE_PAY=false
ENABLE_GOOGLE_PAY=true
ENABLE_LIVE_CHAT=true

# App Configuration
APP_NAME=Alita Ecommerce
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0

# Cache Configuration
CACHE_DURATION_PRODUCTS=86400
CACHE_DURATION_CATEGORIES=86400
CACHE_DURATION_USER_DATA=604800

# Logging
ENABLE_DEBUG_LOGGING=true
ENABLE_CRASH_REPORTING=true

# Content Delivery Network
CDN_BASE_URL=https://cdn.alitaecommerce.com

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key

# Geolocation
MAP_API_KEY=your_map_api_key
ENABLE_LOCATION_SERVICES=true