ENVIRONMENT=production
API_BASE_URL=https://api.alitaecommerce.com/v1
API_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=your_prod_firebase_api_key
FIREBASE_APP_ID=your_prod_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_prod_firebase_messaging_sender_id
FIREBASE_PROJECT_ID=your_prod_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_prod_firebase_storage_bucket

# Payment Gateways Production Keys
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
MIDTRANS_CLIENT_KEY=your_prod_midtrans_client_key
MIDTRANS_SERVER_KEY=your_prod_midtrans_server_key

# Feature Flags
ENABLE_SOCIAL_LOGIN=true
ENABLE_APPLE_PAY=true
ENABLE_GOOGLE_PAY=true
ENABLE_LIVE_CHAT=true
ENABLE_LOCATION_SERVICES=true

# App Configuration
APP_NAME=Alita Ecommerce
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0

# Cache Configuration
CACHE_DURATION_PRODUCTS=86400
CACHE_DURATION_CATEGORIES=86400
CACHE_DURATION_USER_DATA=604800

# Logging
ENABLE_DEBUG_LOGGING=false
ENABLE_CRASH_REPORTING=true

# Content Delivery Network
CDN_BASE_URL=https://cdn.alitaecommerce.com